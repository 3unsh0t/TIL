class Solution {
    public int findGCD(int[] nums) {
        Arrays.sort(nums);
        int smallest = nums[0];
        int largest = nums[nums.length-1];
        
        int result = smallest;

        while (smallest>0){
            if (largest%result == 0 && smallest%result==0)
                break;
            result--;
        }
        return result;
    }
}
